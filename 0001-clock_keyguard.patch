diff --git a/frameworks/base/packages/Keyguard/src/com/android/keyguard/ClockView.java b/frameworks/base/packages/Keyguard/src/com/android/keyguard/ClockView.java
new file mode 100644
index 0000000..02dfd48
--- /dev/null
+++ b/frameworks/base/packages/Keyguard/src/com/android/keyguard/ClockView.java
@@ -0,0 +1,141 @@
+package com.android.keyguard;
+
+import java.util.Calendar;
+
+import android.content.BroadcastReceiver;
+import android.content.Context;
+import android.content.Intent;
+import android.content.IntentFilter;
+import android.os.Handler;
+import android.os.Message;
+import android.util.AttributeSet;
+import android.util.Log;
+import android.view.MotionEvent;
+import android.widget.ImageView;
+import android.widget.RelativeLayout;
+
+/**
+ *
+ * @author : zpa
+ *
+ * @version : 1.0.0
+ *
+ *          2017年12月15日 下午3:14:36
+ *
+ */
+public class ClockView extends RelativeLayout {
+	
+	private static final String TAG="ClockView";
+	private		ImageView img_miniute;
+	private ImageView img_hour;
+	private ImageView img_second;
+	
+	private TimeReceiver mTimeReceiver;
+	private Context mContext ;
+	
+	private float mDownX;
+	private float mDownY;
+
+	private KeyguardHostView mHostView;
+
+	private Handler mHandler=new Handler(){
+		public void handleMessage(Message msg){
+			super.handleMessage(msg);
+			refreshTime();
+			mHandler.removeMessages(0);
+			mHandler.sendEmptyMessageDelayed(0, 1000);
+		}
+	};
+	public ClockView(Context context) {
+		this(context, null);
+	}
+
+	public ClockView(Context context, AttributeSet attrs) {
+		this(context, attrs, 0);
+	}
+
+	public ClockView(Context context, AttributeSet attrs, int def) {
+		super(context, attrs, def);
+		mTimeReceiver=new TimeReceiver();
+		mContext=context;
+
+	}
+	public void setKeyguardHostView(KeyguardHostView hostView){
+		mHostView=hostView;
+	}
+	public boolean onTouchEvent(MotionEvent event){
+		Log.e(TAG,"+++++++++++++++++++>>>>onTouchEvent");
+		Log.e(TAG,"+onTouchEvent++++++++++++++++++>>>>"+event.getAction());
+		switch(event.getAction()){
+		case MotionEvent.ACTION_DOWN:
+
+			mDownX=event.getX();
+			mDownY=event.getY();
+			break;
+		case MotionEvent.ACTION_MOVE:
+		case MotionEvent.ACTION_CANCEL:
+		case MotionEvent.ACTION_UP:
+			Log.e(TAG,"event.getX()++++++++++++++>>>>"+event.getX());
+			Log.e(TAG,"mDownX++++++++++++++>>>>"+mDownX);
+			Log.e(TAG,"event.getY()++++++++++++++>>>>"+event.getY());
+			Log.e(TAG,"mDownY++++++++++++++>>>>"+mDownY);
+			if(event.getX()-mDownX>=20 || event.getY()-mDownY>20){
+				Log.e(TAG,"mHostView++++++++++++++++>>>");
+				mHostView.userActivity();
+			}
+			break;
+		}
+		return super.onTouchEvent(event);
+	}
+
+	public void onFinishInflate(){
+		super.onFinishInflate();
+		img_miniute=(ImageView) findViewById(R.id.img_miniute);
+		img_hour=(ImageView) findViewById(R.id.img_hour);
+		img_second=(ImageView) findViewById(R.id.img_second);
+		mHandler.removeMessages(0);
+		mHandler.sendEmptyMessage(0);
+	}
+
+	
+	public void onAttachedToWindow(){
+		super.onAttachedToWindow();
+		mHandler.removeMessages(0);
+		mHandler.sendEmptyMessage(0);
+
+		IntentFilter filter = new IntentFilter();
+		filter.addAction(Intent.ACTION_TIME_TICK);
+		filter.addAction(Intent.ACTION_TIMEZONE_CHANGED);
+		filter.addAction(Intent.ACTION_TIME_CHANGED);
+		filter.addAction(Intent.ACTION_LOCALE_CHANGED);
+		mContext.registerReceiver(mTimeReceiver, filter);
+	}
+
+	public void onDetachedFromWindow(){
+		super.onDetachedFromWindow();
+		mHandler.removeMessages(0);
+		mContext.unregisterReceiver(mTimeReceiver);
+	}
+	
+	public class TimeReceiver extends BroadcastReceiver{
+
+		@Override
+		public void onReceive(Context context, Intent intent) {
+			mHandler.removeMessages(0);
+			mHandler.sendEmptyMessage(0);
+		}
+		
+	}
+	public void refreshTime(){
+		Calendar calendar = Calendar.getInstance();
+		
+		int hour = calendar.get(Calendar.HOUR);
+		int minute = calendar.get(Calendar.MINUTE);
+		int second = calendar.get(Calendar.SECOND);
+		float hourRotation=(float)hour*360/12+(float)minute*30/60;
+		img_hour.setRotation(hourRotation);
+		img_miniute.setRotation((float)minute*360/60);
+		img_second.setRotation((float)second*360/60);
+		
+	}
+}
